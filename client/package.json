{
  "name": "blackforestgrillpwa",
  "version": "0.0.1",
  "description": "The Black Forest Grill",
  "productName": "Black Forest Grill",
  "author": "Nic <nf.denuzzo@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "build": "quasar build",
    "copy": "copy .\\netlify.toml dist\\pwa\\netlify.toml",
    "build:pwa": "quasar build -m pwa && npm run copy",
    "lint": "eslint --ext .js,.vue ./",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit": "jest --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\""
  },
  "dependencies": {
    "@quasar/extras": "^1.9.13",
    "@quasar/icongenie": "^2.3.3",
    "@types/date-fns": "^2.6.0",
    "auth0-js": "^9.14.0",
    "auth0-lock": "^11.27.2",
    "axios": "^0.21.1",
    "babel-plugin-transform-imports": "^2.0.0",
    "chart.js": "^2.9.4",
    "cleave.js": "^1.6.0",
    "core-js": "^3.8.2",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "idb": "^5.0.8",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.19",
    "logrocket-vuex": "0.0.3",
    "md-gum-polyfill": "^1.0.0",
    "md5": "^2.3.0",
    "qs": "^6.9.4",
    "quasar": "^1.15.0",
    "vue-chartjs": "^3.5.1",
    "vue-draggable": "2.0.0-beta.0",
    "vue-i18n": "^8.22.3",
    "vue-image-upload-resize": "^2.3.0",
    "vuex-persistedstate": "^3.0.1"
  },
  "devDependencies": {
    "@quasar/app": "^2.2.7",
    "@quasar/quasar-app-extension-testing": "^1.0.3",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.0.1",
    "@vue/test-utils": "^1.1.2",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-vue": "^6.1.2",
    "strip-ansi": "=3.0.1",
    "vue-jest": "^3.0.7",
    "workbox-webpack-plugin": "^5.1.4"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest"
    }
  },
  "browserslist": [
    "ie >= 11",
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
